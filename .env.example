# Server
PORT=3000
NODE_ENV=development
CORS_ORIGINS=http://localhost:3000
API_KEY=knowWHERE-ak-XXXXXXXXXXXXXXXXXXXX
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=knowwhere
DATABASE_URL=postgresql://knowwhere_admin:knowwhere_admin_pass@localhost:5432/knowwhere

DB_SUPERUSER=knowwhere_superadmin
DB_SUPERPASS=knowwhere_superadmin_pass

DB_ADMIN_USER=knowwhere_admin
DB_ADMIN_PASS=knowwhere_admin_pass

DB_INGEST_USER=knowwhere_ingest
DB_INGEST_PASS=knowwhere_ingest_pass

DB_READER_USER=knowwhere_reader
DB_READER_PASS=knowwhere_reader_pass

# Embeddings / reranker
EMBEDDING_ENDPOINT=http://localhost:8081/embed
EMBEDDING_MODEL=bge-base-en-v1.5
RERANK_ENDPOINT=http://localhost:8082/rerank

# OAuth & API key service
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
AUTH_COOKIE_SECRET=change-me-long-random
CONFIG_ENC_KEY=change-me-32-bytes
MASTER_CONFIG_KEY=change-me-master-config

# MongoDB for auth/api keys
MONGODB_URI=mongodb+srv://<db_username>:<db_password>@<app_name>.<app_id>.mongodb.net/?appName=<app_name>
MONGODB_DB=knowwhere_mongo
